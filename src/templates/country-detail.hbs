{{#each this}}
<h1 class="country">{{name}}</h1>
<div class="box">
    <div class="box-info">
        <p class="capital"><b>Capital:</b> {{capital}}</p>
        <p class="population"><b>Population:</b> {{population}}</p>
        <ul class="languages-list"><b>Languages:</b>
            {{#each languages}}
            <li type="disc" class="languages-list__item">
                {{this.name}}
            </li>
            {{/each}}
        </ul>
    </div>
    <div class="box-flag">
        <img class="flag-img" src="{{flag}}" alt="">
    </div>
</div>
{{/each}}